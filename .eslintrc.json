{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-shadow": "off",
    "no-console": 1,
    "no-use-before-define": 1,
    "import/extensions": [0, { "<js>": "always" }],
    "no-underscore-dangle": ["error", { "allow": ["_id"] }]
  }
}
